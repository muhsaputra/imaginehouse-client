{
  "redirects": [
    {
      "source": "/login",
      "destination": "https://imaginehouse-dashboard.vercel.app/",
      "permanent": false
    },
    {
      "source": "/dashboard/(.*)",
      "destination": "https://imaginehouse-dashboard.vercel.app/dashboard/$1",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
